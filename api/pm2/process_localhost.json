{
  "name" : "es-db-api",
  "script" : "app.local.js",
  "instances": "max",
  "exec_mode": "cluster_mode",
  "node_args": "--max_old_space_size=256",
  "max_memory_restart" : "200M",
  "env_localhost" : {
    "ELASTICSEARCH_URI": "http://localhost:19200",
    "ELASTICSEARCH_INDEX": "favirote",
    "ELASTICSEARCH_VERSION": "v1",
    "X_API_KEY": "x-api-key",
    "X_REQUEST_ID": "x-request-id",
    "NODE_ENV": "localhost",
    "DEBUG_FLAG": true
  }
}
